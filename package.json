{
  "name": "aria-fs",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "ts-node -O '{ \"module\": \"commonjs\", \"lib\": [ \"es2015\" ] }' build.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{ \"lib\": [ \"es2015\" ] }' nyc --reporter=lcov --reporter=text-summary mocha --require ts-node/register/type-check src/file.spec.ts",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aelbore/aria-fs.git"
  },
  "keywords": [],
  "author": "Arjay Elbore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aelbore/aria-fs/issues"
  },
  "homepage": "https://github.com/aelbore/aria-fs#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^10.12.18",
    "@types/sinon": "^7.0.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mock-fs": "^4.7.0",
    "nyc": "^13.1.0",
    "rollup": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "sinon": "^7.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "minimatch": "^3.0.4"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "functions": 50,
    "branches": 50,
    "exclude": [
      "**/*.d.ts",
      "src/**/*.spec.ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "require": [
      "ts-node/register/type-check",
      "source-map-support/register"
    ],
    "extension": [
      ".ts"
    ],
    "cache": true,
    "all": true
  }
}
